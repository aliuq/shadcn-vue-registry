{
  "name": "ai-elements-vue",
  "version": "1.2.2",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=9.6.0"
  },
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "dev": "turbo run dev",
    "dev:registry": "pnpm --filter registry dev",
    "build:registry": "pnpm --filter registry build",
    "prepare": "pnpm simple-git-hooks",
    "lint": "markdownlint . && eslint . && turbo run lint",
    "lint:fix": "eslint . --fix",
    "bumpp": "bumpp package.json packages/*/package.json"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.4.2",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "lint-staged": "^16.2.7",
    "markdownlint-cli": "catalog:",
    "simple-git-hooks": "^2.13.1",
    "turbo": "^2.6.3",
    "typescript": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint --edit ${1}"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
